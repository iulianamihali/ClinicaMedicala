         <font>
            <Font name="Times New Roman" size="19.0" />
         </font>
      </Label>
      <ImageView fitHeight="43.0" fitWidth="45.0" layoutX="31.0" layoutY="157.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/opinieRecenzie.png" />
