<AnchorPane fx:id="scenaRecenzii" prefHeight="500.0" prefWidth="750.0" stylesheets="@../css/scenaRecenzii.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ScenaRecenziiController">
