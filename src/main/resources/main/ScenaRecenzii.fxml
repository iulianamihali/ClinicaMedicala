      </ImageView>
      <ImageView fitHeight="155.0" fitWidth="162.0" layoutX="541.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/backgroundRecenzie.png" />
